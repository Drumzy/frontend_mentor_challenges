{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@images/*": [
        "src/assets/images/*"
      ],
      "@components/*": ["src/components/*"],
      "@utilities/*":["src/utilities/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}